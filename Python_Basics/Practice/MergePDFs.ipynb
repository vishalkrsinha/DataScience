    "from PyPDF2 import PdfFileMerger\n",
    "\n",
    "pdfs = ['0.pdf','1.pdf', '3.pdf', '4.pdf','5.pdf','6.pdf','7.pdf','8.pdf','9.pdf','10.pdf']\n",
    "\n",
    "merger = PdfFileMerger()\n",
    "\n",
    "for pdf in pdfs:\n",
    "    merger.append(pdf)\n",
    "\n",
    "merger.write(\"CitiBank_Complain.pdf\")\n",
    "merger.close()"
   ]
